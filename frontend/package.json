{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"preview": "svelte-kit preview",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"prepare": "npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark"
	},
	"devDependencies": {
		"@smui/button": "^5.0.0-beta.6",
		"@smui/card": "^5.0.0-beta.6",
		"@smui/checkbox": "^6.0.0-beta.15",
		"@smui/dialog": "^5.0.0-beta.7",
		"@smui/drawer": "^5.0.0-beta.7",
		"@smui/layout-grid": "^5.0.0-beta.7",
		"@smui/radio": "^5.0.0-beta.7",
		"@smui/snackbar": "^5.0.0-beta.7",
		"@smui/textfield": "^5.0.0-beta.7",
		"@sveltejs/adapter-node": "^1.0.0-next.55",
		"@sveltejs/kit": "next",
		"autoprefixer": "^10.3.4",
		"cssnano": "^5.0.8",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"smui-theme": "^5.0.0-beta.6",
		"svelte": "^3.34.0",
		"svelte-clipboard": "^1.0.0",
		"svelte-preprocess": "^4.9.4",
		"tailwindcss": "^2.2.16"
	},
	"type": "module",
	"dependencies": {
		"@fontsource/fira-mono": "^4.5.0",
		"@lukeed/uuid": "^2.0.0",
		"cookie": "^0.4.1",
		"dotenv": "^10.0.0",
		"socket.io": "^4.2.0",
		"socket.io-client": "^4.2.0",
		"svelte-material-ui": "^5.0.0-beta.6",
		"svelte-preprocess": "^4.9.5",
		"typescript": "^4.4.3",
		"uuid": "^8.3.2"
	}
}
